<UserControl x:Class="Framefield.Tooll.RenderUpdateDebugViz"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" VerticalAlignment="Bottom">
    <Canvas IsHitTestVisible="False" VerticalAlignment="Bottom">
        <Canvas.CacheMode>
            <BitmapCache></BitmapCache>
        </Canvas.CacheMode>
        <Rectangle x:Name="XRect" Fill="Orange" Width="2" Height="10" Canvas.Left="0" Canvas.Top="-4"></Rectangle>
            
    </Canvas>
</UserControl>
